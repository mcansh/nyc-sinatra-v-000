<h1>Create a Landmark</h1>

<form action="/landmarks" method="post">
  <fieldset>
    <label for="landmark_name">Landmark Name</label>
    <input id="landmark_name" type="text" name="landmark[name]" placeholder="name">
  </fieldset>

  <fieldset>
    <label for="landmark_year_completed">Landmark Year Completed</label>
    <input id="landmark_year_completed" type="text" name="landmark[year_completed]" placeholder="year completed">
  </fieldset>

  <fieldset>
    <input type="submit" value="Create New Landmark">
  </fieldset>
</form>
